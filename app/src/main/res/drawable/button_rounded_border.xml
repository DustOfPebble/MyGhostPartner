<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="3dip" android:color="#ffff2a00" />
            <gradient  android:angle="-90"  android:startColor="#c7d6201a" android:endColor="#8ce04a24"  />
       </shape>
    </item>
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="3dip" android:color="#e0cacaca" />
            <gradient  android:angle="-90"  android:startColor="#a0828282" android:endColor="#bd000000"  />
        </shape>
    </item>
</selector>